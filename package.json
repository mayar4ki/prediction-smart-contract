{
  "name": "prediction",
  "version": "1.0.0",
  "type": "module",
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^4.0.0",
    "@nomicfoundation/hardhat-ignition": "^3.0.0",
    "@nomicfoundation/hardhat-ignition-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition-viem": "^3.0.0",
    "@nomicfoundation/hardhat-keystore": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^3.0.0",
    "@nomicfoundation/hardhat-node-test-runner": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^3.0.0",
    "@types/node": "^22.17.2",
    "dotenv-cli": "^10.0.0",
    "ethers": "^6.15.0",
    "forge-std": "github:foundry-rs/forge-std#v1.9.4",
    "hardhat": "^3.0.0",
    "typescript": "~5.8.0"
  },
  "scripts": {
    "deploy:sepolia": "dotenv -e .env.eth.sepolia -- hardhat run scripts/deploy.ts --network sepolia",
    "verify:sepolia": "node scripts/generateArgs.cjs ignition/deployments/sepolia/journal.jsonl && dotenv -e .env.eth.sepolia -- hardhat verify --network sepolia --constructor-args-path ignition/deployments/sepolia/constructor-args.ts",
    "visualize": "hardhat ignition visualize ./ignition/modules/AiPredictionV1.ts",
    "clear": "rm -rf cache/ artifacts/ ignition/deployments/"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "dependencies": {
    "@chainlink/contracts": "^1.4.0",
    "@chainlink/functions-toolkit": "^0.3.2",
    "@openzeppelin/contracts": "^5.4.0"
  }
}
